{
  "name": "papillon-explorer-client",
  "version": "0.0.0",
  "description": "Papillon Explorer Client",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --progress --hide-modules",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm install",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "uninstall-mts-scripts": "npm un -D mts-scripts",
    "install-mts-scripts": "npm i -D mts-scripts",
    "reinstall-mts-scripts": "npm-run-all uninstall-mts-scripts install-mts-scripts",
    "test": "echo \"No test specified\" && exit 0",
    "remove-packages": "rm -rf node_modules package-lock.json",
    "ci": "npm-run-all test"
  },
  "license": "MIT",
  "devDependencies": {
    "mts-scripts": "0.0.15"
  },
  "dependencies": {
    "papillon-alerts": "^1.0.41",
    "papillon-avatars": "^1.0.41",
    "papillon-blankslate": "^1.0.41",
    "papillon-breadcrumb": "^1.0.41",
    "papillon-buttons": "^1.0.41",
    "papillon-labels": "^1.0.43",
    "papillon-layout": "^1.0.41",
    "papillon-navigation": "^1.0.41",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1"
  }
}
