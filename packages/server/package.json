{
  "name": "papillon-explorer-server",
  "version": "0.0.0",
  "description": "Papillon Explorer Server",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm install",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "uninstall-mts-scripts": "npm un -D mts-scripts",
    "install-mts-scripts": "npm i -D mts-scripts",
    "reinstall-mts-scripts": "npm-run-all uninstall-mts-scripts install-mts-scripts",
    "test": "echo \"No test specified\" && exit 0",
    "remove-packages": "rm -rf node_modules package-lock.json",
    "ci": "npm-run-all test"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "mts-scripts": "0.0.11",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "uuid": "^3.3.2"
  }
}
